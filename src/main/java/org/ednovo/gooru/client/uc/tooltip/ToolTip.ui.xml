<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:panel="urn:import:org.ednovo.gooru.client.ui">
	<ui:style field="toolTip" src="ToolTip.css" />
		
	<g:HTMLPanel styleName="{toolTip.tooltipContainer}"
		ui:field="panelCode">
		<!-- <g:HTMLPanel styleName="{toolTip.arrowBorder}"></g:HTMLPanel> -->
		<g:HTMLPanel styleName="{toolTip.arrow}"></g:HTMLPanel>
		<g:HTMLPanel styleName="{toolTip.tooltipContent}" ui:field="tootltipContent">
			<g:HTMLPanel styleName="{toolTip.tooltipContentTitle}"
				ui:field="lblTitle" />
			<g:Anchor ui:field="lblLink"></g:Anchor>
		</g:HTMLPanel>
	</g:HTMLPanel>

</ui:UiBinder> 