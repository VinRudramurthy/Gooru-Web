<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:col="urn:import:org.ednovo.gooru.client.mvp.shelf.collection"
	xmlns:uc="urn:import:org.ednovo.gooru.client.uc">
	 <ui:style field= "editQuestionStyle" src="EditQuestionResource.css"></ui:style>
 	 <g:HTMLPanel styleName="{editQuestionStyle.addResourceThumbimgContainer}" >
               <g:HTMLPanel styleName="{editQuestionStyle.addResourceThumbimgHeading}" ui:field="questionImageText"></g:HTMLPanel>
               <g:HTMLPanel styleName="{editQuestionStyle.addResourceThumbimg}" >                     
           		<g:Image styleName="{editQuestionStyle.addResourceThumbimg}" ui:field="updateQuestionImage"></g:Image>
               </g:HTMLPanel>
               <g:HTMLPanel styleName="{editQuestionStyle.addResourceThumbimgLeftContent}" >
                   <g:HTMLPanel styleName="{editQuestionStyle.addResourceThumbimgLeftContainer}" >
                       <g:Label styleName="{editQuestionStyle.addResourceEditThumbimgLeft}" ui:field="uploadImage"></g:Label>
                       <g:Label styleName="{editQuestionStyle.addResourceEditThumbimgLeft}" ui:field="removeImage"></g:Label>
                   </g:HTMLPanel>
               </g:HTMLPanel>
           </g:HTMLPanel> 
</ui:UiBinder> 