<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
<ui:style field="PPPStyle" src="profile-page-Following-style.css" />
	<g:HTMLPanel styleName="{PPPStyle.popup} {PPPStyle.userInput}">
			<g:HTMLPanel styleName="{PPPStyle.popupInner}">
				<g:Label ui:field="popupHeaderTitleLbl" styleName="{PPPStyle.popupHeader}"></g:Label>
                <g:HTMLPanel styleName="{PPPStyle.popupContent}">
                     <g:InlineLabel ui:field="inputTitleLbl" styleName="{PPPStyle.inputLabel}"></g:InlineLabel>
                     <g:InlineLabel ui:field="inputTitleLblName" styleName="{PPPStyle.inputLabel}"></g:InlineLabel>
                  </g:HTMLPanel>
                  <g:HTMLPanel styleName="{PPPStyle.okCancel}" ui:field="buttonsContainer">
                      <g:Button styleName="primary {PPPStyle.buttonMargins}" ui:field="okBtn"></g:Button>
                </g:HTMLPanel>
            </g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 