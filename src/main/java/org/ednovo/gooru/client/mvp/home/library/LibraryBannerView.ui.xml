<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:style field="libraryStyle" src="library.css" type="org.ednovo.gooru.client.mvp.home.library.LibraryStyleBundle" />
	<g:HTMLPanel ui:field="landingBannerInner">
	    <g:HTMLPanel styleName="{libraryStyle.headerPanel}">
		    <g:HTMLPanel ui:field="partnerLogo"/>
			<g:Label styleName="{libraryStyle.header}" ui:field="headerTag"></g:Label>
			<g:Label styleName="{libraryStyle.subheader}" ui:field="subHeaderTag"></g:Label>
<!-- 			<g:Button styleName="primary {libraryStyle.signUpButton}" ui:field="btnSignUp"></g:Button>
 -->	    </g:HTMLPanel>
		<g:HTMLPanel styleName="{libraryStyle.fourSteps}" ui:field="fourSteps">
			<g:HTMLPanel styleName="{libraryStyle.step}">
				<g:HTMLPanel styleName="{libraryStyle.libraryBannerImage}">
					<g:HTMLPanel styleName="{libraryStyle.find}" />
				</g:HTMLPanel>
				<g:HTMLPanel styleName="{libraryStyle.libraryBannerImageData}" ui:field="findLbl"/>
			</g:HTMLPanel>
			<g:HTMLPanel styleName="{libraryStyle.step}">
				<g:HTMLPanel styleName="{libraryStyle.libraryBannerImage}">
					<g:HTMLPanel styleName="{libraryStyle.share}"></g:HTMLPanel>
				</g:HTMLPanel>
				<g:HTMLPanel styleName="{libraryStyle.libraryBannerImageData}" ui:field="shareLbl"/>
			</g:HTMLPanel>
			<g:HTMLPanel styleName="{libraryStyle.step}">
				<g:HTMLPanel styleName="{libraryStyle.libraryBannerImage}">
					<g:HTMLPanel styleName="{libraryStyle.measure}"></g:HTMLPanel>
				</g:HTMLPanel>
				<g:HTMLPanel styleName="{libraryStyle.libraryBannerImageData}" ui:field="measureLbl"/>
			</g:HTMLPanel>
			<g:HTMLPanel styleName="{libraryStyle.last} {libraryStyle.step}">
				<g:HTMLPanel styleName="{libraryStyle.libraryBannerImage}">
					<g:HTMLPanel styleName="{libraryStyle.contribute}"></g:HTMLPanel>
				</g:HTMLPanel>
				<g:HTMLPanel styleName="{libraryStyle.libraryBannerImageData}" ui:field="contributeLbl"/>
			</g:HTMLPanel>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 