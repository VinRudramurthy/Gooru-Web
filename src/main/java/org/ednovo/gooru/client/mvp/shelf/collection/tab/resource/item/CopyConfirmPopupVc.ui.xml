<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:col="urn:import:org.ednovo.gooru.client.mvp.shelf.collection"
	xmlns:uc="urn:import:org.ednovo.gooru.client.uc" xmlns:gg="urn:import:org.ednovo.gooru.client.uc">
	<ui:with type="org.ednovo.gooru.client.mvp.classpages.assignments.AddAssignmentContainerCBundle" field="res"></ui:with>
		<g:FlowPanel styleName="{res.css.popupContent}">
			<g:HTMLPanel styleName="{res.css.popupHeader}">
				<g:Label styleName="{res.css.popupHeaderText}" ui:field="copyCollectionPopupHeader"></g:Label>
			</g:HTMLPanel>
			<g:HTMLPanel styleName="{res.css.assignmentMainContainer}">
				<g:HTMLPanel styleName="{res.css.assignmentContentContainer}">
					<g:HTMLPanel styleName="{res.css.assignmentControlsContainer}">
						<g:Label styleName="{res.css.assignmentTitleLabel}" ui:field="copyResourceTitleLabel"></g:Label>
						<g:FlowPanel styleName="{res.css.dropdowncontainer}">
			              	<g:Label ui:field="dropdownListPlaceHolder" styleName="{res.css.dropdownPlaceHolder}"> </g:Label>
			              	<g:ScrollPanel ui:field="dropdownListContainerScrollPanel" styleName="{res.css.dropdownListContainerScrollPanel}">
				            	<g:HTMLPanel ui:field="floderTreeContainer" styleName="{res.css.folderStucture}">
						   		 </g:HTMLPanel>
					   		 </g:ScrollPanel>
          			   </g:FlowPanel>
          			   <g:Label styleName="{res.css.assignmentErrorMessage}" ui:field="chooseCollectionErrorLabel"></g:Label>
					</g:HTMLPanel>
				</g:HTMLPanel>
	
		</g:HTMLPanel>
					<g:HTMLPanel styleName="{res.css.assignmentButtonsContainerCopy}">
					<g:Button styleName="secondary"  ui:field="cancelResourcePopupBtnLbl" />
					<g:Button styleName="primary"  ui:field="copyResourceBtnLbl" />
					<g:Label ui:field="addingText" styleName=""></g:Label>
				</g:HTMLPanel>
	</g:FlowPanel>
</ui:UiBinder> 